{% extends '@layout/two_cols.html.twig' %}

{% set title = "Gallery" %}

{% block side %}
    {% include '@component/game_menu.html.twig' with {'games': games}  %}
{% endblock %}

{% block content %}
    {% block gallery %}
        {% for post in posts %}
            {% include '@component/card.html.twig' with {'post': post}  %}
        {% endfor %}
    {%  endblock %}
    <button id="getMore" onclick="getMore(this.dataset.lastid, this)" data-lastid="{{ last_id }}">GET MORE</button>
{% endblock %}